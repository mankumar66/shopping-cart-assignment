<section>
    <div id="sliderContainer">
        {{#each sliderContent}}
        <div tabindex="0" class="slider">
            <img data-index="{{id}}" class="slide" alt="{{bannerImageAlt}}" src="{{bannerImageUrl}}" />
        </div>
        {{/each}}
        <a id="arrow-left" href="#">{{constants.PREV_LABEL}}</a>
        <a id="arrow-right" href="#">{{constants.NEXT_LABEL}}</a>
        <div class="dots-wrapper">
            {{#each sliderContent}}
            <span tabindex="0" class="dot"></span>
            {{/each}}
        </div>
    </div>
</section>
<section>
    {{#each categoryContent }}
    <div class="categoryItem">
        <img tabindex="0" data-index="{{id}}" class="category" alt="{{key}}" src="{{imageUrl}}" />
        <div>
            <span tabindex="0">{{name}}</span>
            <span tabindex="0">{{description}}</span>
            <span><a href="">Explore {{key}}</a></span>
        </div>
    </div>
    {{/each}}
</section>